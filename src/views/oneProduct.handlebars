<h1>Producto</h1>
<section>
    <div class="card mb-3" style="max-width: 540px;">
        {{#each payload}}
        <div class="row g-0">
            <div class="col-md-4">
                <img src={{this.thumbnails.[0]}} class="img-fluid rounded-start" alt={{this.description}}>
            </div>
            <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title">{{this.title}}</h5>
                    <p class="card-text">{{this.price}}</p>
                    <p class="card-text"><small class="text-body-secondary">{{this.description}}</small></p>
                </div>
                    <a href="#" class="btn btn-success">Agregar</a>
            </div>
        </div>
            {{/each}}
    </div>
</section>

<script src="/js/product.js"></script>